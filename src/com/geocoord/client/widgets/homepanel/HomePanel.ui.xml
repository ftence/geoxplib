<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
             
  <ui:with field='images'    type='com.geocoord.client.widgets.homepanel.i18n.images.HomePanelImages' />
  <ui:with field='messages'  type='com.geocoord.client.widgets.homepanel.i18n.images.HomePanelMessages' />
  <ui:with field='resources' type='com.geocoord.client.widgets.homepanel.HomePanel.Resources' />
  
  <g:FlowPanel ui:field='root' stylePrimaryName='{resources.css.poiPanelRoot}' >
    <g:Button ui:field='add' stylePrimaryName='{resources.css.poiPanelAdd}' />
  </g:FlowPanel>
</ui:UiBinder>